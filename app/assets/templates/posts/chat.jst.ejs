<a href="javascript:void(0)" data-id="<%= post.escape('blog_id') %>" class="blogname-link">
  <figure class="blog-avatar">
    <img src="<%= post.get('avatar_url') %>">
  </figure>
</a>
<div class="post-content">
  <div class="arrow-left"></div>
   <div class="content-box">
    <div class="post-header"></div>

    <h2 class="post-title"><%= post.escape('title') %></h2>
    <p class="post-body"><%= post.get('parsedBody') %></p>
  </div>

  <div class="post-bottom-bar"></div>
</div>
